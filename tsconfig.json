{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "target": "es2022",
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "baseUrl": ".",
    "esModuleInterop": true
  },
  "compileOnSave": true,
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}
